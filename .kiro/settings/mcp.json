{
  "mcpServers": {
    "aws-docs": {
      "command": "uvx",
      "args": [
        "awslabs.aws-documentation-mcp-server@latest"
      ],
      "env": {
        "FASTMCP_LOG_LEVEL": "ERROR"
      },
      "disabled": false,
      "autoApprove": []
    },
    "serena": {
      "command": "uvx",
      "args": [
        "serena-mcp"
      ],
      "env": {},
      "disabled": false,
      "autoApprove": []
    },
    "sqlite-context": {
      "command": "uvx",
      "args": [
        "mcp-server-sqlite",
        "--db-path",
        ".kiro/context.db"
      ],
      "env": {
        "FASTMCP_LOG_LEVEL": "ERROR"
      },
      "disabled": false,
      "autoApprove": []
    },
    "Context7": {
      "command": "npx",
      "args": [
        "-y",
        "@upstash/context7-mcp"
      ],
      "env": {},
      "disabled": false,
      "autoApprove": []
    }
  }
}